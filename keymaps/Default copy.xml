<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateDatabaseConsoleToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="EditorDown">
    <keyboard-shortcut first-keystroke="down" />
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="up" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
    <keyboard-shortcut first-keystroke="shift ctrl p" />
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="ParameterInfo">
    <keyboard-shortcut first-keystroke="shift ctrl alt p" />
  </action>
  <action id="QuickJavaDoc">
    <keyboard-shortcut first-keystroke="ctrl q" />
    <mouse-shortcut keystroke="alt button2" />
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="Replace" />
  <action id="SelectNextOccurrence" />
</keymap>